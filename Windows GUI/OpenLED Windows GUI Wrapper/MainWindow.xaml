<Window x:Class="OpenLED_Windows_GUI_Wrapper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenLED_Windows_GUI_Wrapper"
        xmlns:svl="clr-namespace:Sound_Library;assembly=Sound_Library"
		mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
	<svl:Spectrum
					BarCount="500"
					BarVisibility="Visible"
							
					MinimumFrequency="0"
					MaximumFrequency="2000"
							
					ThresholdWindow="3"
					ThresholdThreshold="0.1"
					ThresholdInfluence=".5"
							
					BlendedFrames="5"/>
</Window>
